{
  "plugins": [
    "node"
  ],
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "strict": [0],
    "camelcase": [0],
    "arrow-body-style": [0],
    "consistent-return": [0],
    "prefer-const": [2, {"ignoreReadBeforeAssign": true}],
    "comma-dangle": [2, {"functions": "never"}],
    "dot-notation": [0],
    "no-plusplus": [0],
    "prefer-template": [0],
    "yoda": [0],
    "guard-for-in": [0],
    "max-len":["error", 120]
  }
}
